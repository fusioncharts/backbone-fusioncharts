{
  "name": "backbone-fusioncharts",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Simple and Lightweight Backbone wrapper for FusionCharts JavaScript Charting Library",
  "author": {
    "name": "FusionCharts",
    "email": "support@fusioncharts.com"
  },
  "contributors": [
